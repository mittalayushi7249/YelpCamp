<%- include('partials/header') %>


    <div style="width: 30%;margin: 50px auto;border: 1px solid #C0C0C0;padding: 10px;">
        <h1 style="text-align:center;">Registration</h1>
        <form action="/register" method="POST">
            <div class="form-group">
                <label for="title">Username</label>
                <input id="title" class="form-control" type="text" placeholder="Username" name="username">
            </div>
            <div class="form-group">
                <label for="image">Password</label>
                <input id="image" class="form-control" type="password" placeholder="password" name="password">
            </div>
            <input class="form-control btn btn-primary" type="submit">
        </form>
        <a href="/">Go Back</a>

    </div>


<%- include('partials/footer') %>